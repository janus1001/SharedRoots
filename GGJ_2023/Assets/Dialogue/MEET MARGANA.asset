  m_Name: MEET MARGANA
  m_EditorClassIdentifier: 
  dialogueEntries:
  - text: IN THE MORNING, AN OLD WOMAN APPROACHES...
    characterName: 
    characterImage: {fileID: 21300000, guid: b498cad09f0044a47933e7b036841863, type: 3}
  - text: Hello, what are you doing here?
    characterName: 
    characterImage: {fileID: 21300000, guid: 61045215a35635447b9cda80ed0a28c4, type: 3}
  - text: I'm new here. I've travelled far.  I've no home and no money, so I had
      to sleep in the forest.
    characterName: 
    characterImage: {fileID: 21300000, guid: 83a30d2d7ffe2214481923f31cd650ab, type: 3}
  - text: I know just the thing. Follow me!
    characterName: 
    characterImage: {fileID: 21300000, guid: 61045215a35635447b9cda80ed0a28c4, type: 3}
  - text: 'THE OLD WOMAN LEADS MARGANA THROUGH THE FOREST. '
    characterName: 
    characterImage: {fileID: 21300000, guid: b498cad09f0044a47933e7b036841863, type: 3}
  - text: THEY ARRIVE AT THE ENTRANCE TO AN ANCIENT STONE CIRCLE.
    characterName: 
    characterImage: {fileID: 21300000, guid: b498cad09f0044a47933e7b036841863, type: 3}
  navigateToScene: FirstPuzzle
